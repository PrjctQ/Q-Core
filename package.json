{
  "name": "@prjq/q-core",
  "version": "0.0.0-alpha.2",
  "description": "A typescript meta framework for instant, scalable APIs. Designed to eliminate hours of boilerplate and enhance developer experience for scalable backend systems.",
  "main": "dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pack:dry-run": "npm pack --dry-run",
    "test": "vitest",
    "prepublishOnly": "npm run test && npm run lint && npm run build",
    "clean": "rm -rf dist node_modules pnpm-lock.yaml && pnpm install"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "express",
    "meta-framework",
    "crud",
    "api",
    "zod"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/PrjctQ/Q-Core"
  },
  "author": "Zaib Khan kmjahanzaib@proton.me",
  "license": "GPL-3.0",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bytes": "^3.1.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "jsonwebtoken": "^9.0.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.0.17"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "packageManager": "pnpm@10.15.0",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@eslint/json": "^0.13.1",
    "@types/bcrypt": "^6.0.0",
    "@types/bytes": "^3.1.5",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "eslint": "^9.33.0",
    "eslint-js": "github:eslint/js",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "prisma": "^6.14.0",
    "@prisma/client": "^6.14.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  },
  "peerDependencies": {
    "@prisma/client": "^6.14.0"
  }
}
