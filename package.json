{
  "name": "@prjq/q-core",
  "version": "0.0.0-alpha.7",
  "description": "A typescript meta framework for instant, scalable APIs. Designed to eliminate hours of boilerplate and enhance developer experience for scalable backend systems.",
  "main": "dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsc --watch && tsc-alias --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "pack:dry-run": "pnpm pack --dry-run",
    "prepublishOnly": "pnpm test && pnpm lint && pnpm build",
    "clean": "rm -rf dist node_modules pnpm-lock.yaml && pnpm install && pnpm prisma db push"
  },
  "files": [
    "dist/**/*",
    "LICENSE"
  ],
  "keywords": [
    "express",
    "meta-framework",
    "crud",
    "api",
    "zod"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/PrjctQ/Q-Core"
  },
  "author": {
    "name": "Zaib Khan",
    "url": "https://github.com/ZaibKhan0"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bytes": "^3.1.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "jsonwebtoken": "^9.0.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.0.17"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "packageManager": "pnpm@10.15.0",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@eslint/json": "^0.13.1",
    "@prisma/client": "^6.14.0",
    "@types/bcrypt": "^6.0.0",
    "@types/bytes": "^3.1.5",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/runner": "^3.2.4",
    "@vitest/ui": "3.2.4",
    "eslint": "^9.33.0",
    "eslint-js": "github:eslint/js",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "prisma": "^6.14.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@prisma/client": "^6.14.0"
  }
}
